{
  "concurrency": 100,
  "db_conn_str": "root:@tcp(127.0.0.1:4000)/test?parseTime=true&interpolateParams=true&timeout=3s&readTimeout=3s&writeTimeout=3s",
  "use_transaction": true,
  "rate_per_thread": 50,
  "connection_type": "long",
  "templates": [
    {
      "sql": "SELECT id from sbtest? WHERE id in (?)",
      "params": [
        {
          "type": "number",
          "random_mode": "uniform",
          "min": 1,
          "max": 10,
          "comment": "50 is the number of tables"
        },
        {
          "type": "number",
          "random_mode": "uniform",
          "min": 1,
          "max": 10000,
          "comment": "1000000 is the number of rows per table"
        }
      ]
    }
  ]
}